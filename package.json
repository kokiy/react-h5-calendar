{
  "name": "react-h5-calendar",
  "version": "1.0.1",
  "description": "react-h5-calendar React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.less": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0"
  },
  "peerDependencies": {
    "normalize.css": "8.x",
    "react": "16.x"
  },
  "devDependencies": {
    "nwb": "0.25.x",
    "nwb-less": "^0.9.0",
    "prop-types": "15.7.x",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@umijs/fabric": "^2.3.1",
    "conventional-changelog-cli": "^2.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2"
  },
  "author": "kokiy",
  "homepage": "https://github.com/kokiy/react-h5-calendar",
  "license": "MIT",
  "repository": "https://github.com/kokiy/react-h5-calendar",
  "keywords": [
    "react-component","react-h5-calendar","react-mobile-calendar"
  ],
  "dependencies": {
    "dayjs": "1.9.x",
    "normalize.css": "^8.0.1"
  }
}
